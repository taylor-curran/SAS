# OpenLineage Configuration
OPENLINEAGE_URL=http://localhost:5000
OPENLINEAGE_API_KEY=your_openlineage_api_key_here
OPENLINEAGE_NAMESPACE=clinical_data_pipeline

# OpenMetadata Configuration
OPENMETADATA_JWT_TOKEN=your_openmetadata_jwt_token_here
OPENMETADATA_API_ENDPOINT=http://localhost:8585/api

# Airflow Lineage Backend Configuration
AIRFLOW__LINEAGE__BACKEND=airflow_provider_openmetadata.lineage.backend.OpenMetadataLineageBackend
AIRFLOW__LINEAGE__AIRFLOW_SERVICE_NAME=clinical_data_pipeline
AIRFLOW__LINEAGE__OPENMETADATA_API_ENDPOINT=http://localhost:8585/api
AIRFLOW__LINEAGE__JWT_TOKEN=your_openmetadata_jwt_token_here
AIRFLOW__LINEAGE__ONLY_KEEP_DAG_LINEAGE=true
AIRFLOW__LINEAGE__MAX_STATUS=10
AIRFLOW__LINEAGE__TIMEOUT=30
AIRFLOW__LINEAGE__RETRY=3
AIRFLOW__LINEAGE__RETRY_WAIT=60

# Airflow Webserver Configuration
AIRFLOW__WEBSERVER__BASE_URL=http://localhost:8080

# Pipeline Configuration
CONFIG_PATH=/config/pipeline_config.yaml
SCRIPTS_DIR=/scripts
TEMP_DIR=/tmp
